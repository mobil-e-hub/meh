{
  "name": "meh",
  "version": "1.0.0",
  "description": "mobil-e-Hub: Intelligent Drone Logistics Network",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "setup:mac": "npm-run-all -s setup:*:mac",
    "start:mac": "npm-run-all -p start:*:mac",
    "setup:mqtt:mac": "npm run setup:mac --prefix mqtt",
    "setup:sim:mac": "npm run setup:mac --prefix sim",
    "setup:opt:mac": "npm run setup:mac --prefix opt",
    "setup:vue:mac": "npm run setup:mac --prefix viz/vue",
    "setup:ana:mac": "npm run setup:mac --prefix ana",
    "start:mqtt:mac": "npm run start:mac --prefix mqtt",
    "start:sim:mac": "npm run start:mac --prefix sim",
    "start:opt:mac": "npm run start:mac --prefix opt",
    "start:vue:mac": "npm run start:mac --prefix viz/vue",
    "start:ana:mac": "npm run start:mac --prefix ana",
    "setup:windows": "npm-run-all -s setup:*:windows",
    "start:windows": "npm-run-all -p start:*:windows",
    "setup:mqtt:windows": "cd mqtt && npm run setup:windows",
    "setup:sim:windows": "cd sim && npm run setup:windows",
    "setup:opt:windows": "cd opt && npm run setup:windows",
    "setup:vue:windows": "cd viz\\vue && npm run setup:windows",
    "setup:ana:windows": "cd ana && npm run setup:windows",
    "start:mqtt:windows": "npm run start:windows --prefix mqtt",
    "start:sim:windows": "npm run start:windows --prefix sim",
    "start:opt:windows": "npm run start:windows --prefix opt",
    "start:vue:windows": "npm run start:windows --prefix viz/vue",
    "start:ana:windows": "npm run start:windows --prefix ana"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  },
  "author": "",
  "license": "ISC",
  "eslintConfig": {
    "rules": {
      "no-undef": "off",
      "no-unused-vars": "off"
    }
  }
}
